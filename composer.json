{
  "name": "jeppos/shopify-php-sdk",
  "description": "An easy to use Shopify PHP SDK.",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^7.1",
    "jms/serializer": "^1.6",
    "guzzlehttp/guzzle": "^6.3",
    "doctrine/collections": "^1.5",
    "consistence/consistence": "^1.0",
    "consistence/consistence-jms-serializer": "^1.0",
    "symfony/expression-language": "^4.1"
  },
  "require-dev": {
    "codacy/coverage": "^1.4",
    "phpunit/phpunit": "^7.3",
    "phpmd/phpmd": "^2.6",
    "squizlabs/php_codesniffer": "^3.3",
    "fzaninotto/faker": "^1.8",
    "vlucas/phpdotenv": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Jeppos\\ShopifySDK\\": "src/Jeppos/ShopifySDK"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Unit\\Jeppos\\ShopifySDK\\": "tests/unit/Jeppos/ShopifySDK",
      "Tests\\Integration\\Jeppos\\ShopifySDK\\": "tests/integration/Jeppos/ShopifySDK"
    }
  }
}
