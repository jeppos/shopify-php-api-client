{
    "name": "jeppos/shopify-php-api-client",
    "description": "A simple Shopify PHP API client.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.1",
        "jms/serializer": "^1.6",
        "guzzlehttp/guzzle": "^6.2",
        "doctrine/common": "^2.7",
        "consistence/consistence-jms-serializer": "^1.0"
    },
    "require-dev": {
        "codacy/coverage": "^1.1",
        "phpunit/phpunit": "^6.1",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Jeppos\\ShopifyApiClient\\": "src/Jeppos/ShopifyApiClient"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Jeppos\\ShopifyApiClient\\": "tests/Jeppos/ShopifyApiClient"
        }
    }
}
